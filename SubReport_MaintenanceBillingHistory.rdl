  <DataSets>
    <DataSet Name="dsBillingHistory">
      <Query>
        <DataSourceName>MMRPTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Contract">
            <Value>=Parameters!Contract.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  Contract_Number,
        Billing_Date = CONVERT(VARCHAR, Wennsoft_Billing_Date, 101),
        Billable = CASE RMDTYPAL
                     WHEN 7 THEN Billable_Subtotal * -1
                    ELSE Billable_Subtotal
                  END

FROM   MM.SV00564_FFSC_Billings (NOLOCK)
WHERE Contract_Number = @Contract
AND Wennsoft_Billing_Date BETWEEN @FromDate AND @ToDate
ORDER BY CONVERT(VARCHAR,Billing_Date, 112)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Contract_Number">
          <DataField>Contract_Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Billing_Date">
          <DataField>Billing_Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Billable">
          <DataField>Billable</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
