  <DataSets>
    <DataSet Name="dsReport">
      <Query>
        <DataSourceName>MMFS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TaskList">
            <Value>=Parameters!TaskList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
      Task_List_Type = RTRIM(SV00591.Task_List_Type)
   , Task_List_Description = RTRIM(SV00591.Task_List_Description)
    , Sub_1 = RTRIM(SV00591.Sub_1)
	, Sub_2 = RTRIM(SV00591.Sub_2)
	, Sub_3 = RTRIM(SV00591.Sub_3)
	, Sub_4 = RTRIM(SV00591.Sub_4)
	, Task_List_ID = RTRIM(SV00591.Task_List_ID) 
	, Task_Code = RTRIM(SV00592.Task_Code  )
	, Task_Description = RTRIM(SV00592.Task_Description)
	, OptionID = RTRIM(ISNULL(O.OptionID, '~') )
	, Estimate_Hours = SV00592.Estimate_Hours * .01
	, RT = CASE SV00560.Coverage_Code 
			WHEN 0 THEN ''
			WHEN 1 THEN 'YN'
			WHEN 2 THEN 'TX'
			WHEN 3 THEN '1N'
			WHEN 4 THEN '2N'
			WHEN 5 THEN '3N'
			WHEN 6 THEN 'DL'
			WHEN 7 THEN 'LT'
			END
, SV00592.Task_Line_Number          
FROM MM.Task_List_Task_Options O
RIGHT JOIN SV00592 -- SV_Task_List_Tasks_MSTR   
	ON O.Task_Code = dbo.SV00592.Task_Code
	AND O.Task_List_ID = dbo.SV00592.Task_List_ID
JOIN SV00591 -- SV_Task_List_MSTR 
	ON dbo.SV00592.Task_List_ID = dbo.SV00591.Task_List_ID 
LEFT JOIN dbo.SV00560
	ON SV00592.Task_Code = SV00560.Task_Code
WHERE SV00591.Task_List_Type  IN (@TaskList)
ORDER BY dbo.SV00592.Task_Description</CommandText>
      </Query>
