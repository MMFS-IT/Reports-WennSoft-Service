      <Query>
        <DataSourceName>MMFS</DataSourceName>
        <CommandText>SELECT 'YN' as Code, 'Yes/No' AS RT
UNION
SELECT '1N' as Code, '1-Number' AS RT
UNION
SELECT '2N' as Code, '2-Numbers' AS RT
UNION
SELECT '3N' as Code, '3-Numbers' AS RT
UNION
SELECT 'TX' as Code, 'Text' AS RT
UNION
SELECT 'DL' as Code, 'DropList' AS RT
UNION
SELECT 'LT' as Code, 'DropList+Text' AS RT</CommandText>
