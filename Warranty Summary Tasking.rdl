  <DataSets>
    <DataSet Name="dsTasking">
      <Query>
        <DataSourceName>MMRPTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ServiceCallID">
            <Value>=Parameters!ServiceCallID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
/************************************************************/
/*THIS SECTION DETAILS THE TASKING SECTION OF THE REPORT.  THE
TASKS WOULD BE LISTED BY EACH SERVICE CALL ID*/
/************************************************************/

SELECT Service_Call_ID,
	Task_Code = RTRIM(Task_Code),
	RTRIM(MAX(CONVERT(VARCHAR(MAX),WS_Response_String))) AS Task_Response,
	RTRIM(MAX(CONVERT(VARCHAR(MAX),WS_Response_Text))) AS Task_Note
FROM dbo.SV00317 (NOLOCK)
WHERE Service_Call_ID = @ServiceCallID
GROUP BY Service_Call_ID, Task_Code </CommandText>
      </Query>
      <Fields>
        <Field Name="Service_Call_ID">
          <DataField>Service_Call_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Task_Code">
          <DataField>Task_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Task_Response">
          <DataField>Task_Response</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Task_Note">
          <DataField>Task_Note</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
